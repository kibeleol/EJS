<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
</head>
<body>
    <h1>Benutzerliste</h1>
    <table border="1">
        <tr>
            <th>Name</th>
            <th>Alter</th>
        </tr>
        <% users.forEach(function(user) { %>
            <tr>
                <td><%= user.name %></td>
                <td><%= user.age %></td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
